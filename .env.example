# AWS Elastic Beanstalk Environment Variables
# Copy this file to .env.local and fill in your values

# Environment
APP_ENV=production
APP_DEBUG=false
LOG_LEVEL=info

# Database Configuration (RDS)
RDS_DB_HOST=your-database-endpoint.region.rds.amazonaws.com
RDS_DB_USER=admin
RDS_DB_PASSWORD=your-secure-password
RDS_DB_NAME=library

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_S3_BUCKET=your-bucket-name

# SNS Configuration
SNS_TOPIC_ARN=arn:aws:sns:us-east-1:YOUR_ACCOUNT_ID:library-notifications
ENABLE_SNS=true

# CloudWatch Configuration
ENABLE_CLOUDWATCH=true
LOG_GROUP=/aws/elasticbeanstalk/library-app
